{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/Privado-Inc/privado-core/tree/main/src/main/resources/ai/privado/rulevalidator/schema/auditCollection.json",
  "title": "Root",
  "type": "object",
  "required": [
    "auditCollection"
  ],
  "additionalProperties": false,
  "properties": {
    "auditCollection": {
      "$id": "#root/auditCollection",
      "title": "auditCollection",
      "type": "array",
      "default": [],
      "items":{
        "$id": "#root/auditCollection/items",
        "title": "Items",
        "type": "object",
        "required": [
          "id",
          "name",
          "patterns"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "$id": "#root/auditCollection/items/id",
            "title": "Id",
            "type": "string",
            "default": "",
            "examples": [
              "AuditCollection.Test"
            ],
            "pattern": "^.*$"
          },
          "name": {
            "$id": "#root/auditCollection/items/name",
            "title": "Name",
            "type": "string",
            "default": "",
            "examples": [
              "Audit collection dependency info"
            ],
            "pattern": "^.*$"
          },
          "patterns": {
            "$id": "#root/auditCollection/items/patterns",
            "title": "Patterns",
            "type": "array",
            "default": [],
            "items":{
              "$id": "#root/auditCollection/items/patterns/items",
              "title": "Items",
              "type": "string",
              "format": "regex",
              "default": "",
              "examples": [
                ".*(SpringMVC).*"
              ],
              "pattern": "^.*$"
            }
          }
        }
      }
    }
  }
}
